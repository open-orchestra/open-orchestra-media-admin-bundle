{
  "name": "open-orchestra/open-orchestra-media-admin-bundle",
  "description": "This bundle provide the backoffice part for the media of OpenOrchestra",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "OpenOrchestra community",
      "homepage": "https://github.com/open-orchestra/open-orchestra-media-admin-bundle/graphs/contributors"
    }
  ],
  "require": {
    "open-orchestra/open-orchestra-cms-bundle": "1.1.*@dev",
    "open-orchestra/open-orchestra-media-bundle": "1.1.*@dev",
    "open-orchestra/open-orchestra-media-file-bundle": "1.1.*@dev",
    "flowjs/flow-php-server": "*",
    "ext-imagick": "*",
    "php-ffmpeg/php-ffmpeg": "~0.6",
    "koala-framework/composer-extra-assets": "2.0.x"
  },
  "require-dev": {
    "open-orchestra/open-orchestra-model-bundle": "1.1.*@dev",
    "phpunit/phpunit" : "4.7.2",
    "psr/log": "1.0.0",
    "codeclimate/php-test-reporter": "dev-master",
    "phake/phake": "2.1.0"
  },
  "config" : {
    "bin-dir" : "bin"
  },
  "autoload": {
    "psr-4": {
      "OpenOrchestra\\MediaAdminModelBundle\\": "MediaAdminModelBundle/",
      "OpenOrchestra\\MediaAdminBundle\\": "MediaAdminBundle/",
      "OpenOrchestra\\MediaAdmin\\": "MediaAdmin/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.1.x-dev"
    },
    "require-npm": {
      "bower": "~1.3",
      "grunt": "~0.4",
      "grunt-cli": "~0.1",
      "grunt-contrib-symlink": "~0.2",
      "grunt-contrib-less": "~1.0.0",
      "grunt-contrib-cssmin": "~0.7",
      "grunt-contrib-coffee": "~0.8",
      "grunt-contrib-uglify": "~0.2",
      "grunt-contrib-concat": "~0.3",
      "grunt-contrib-watch": "~0.4",
      "grunt-contrib-clean": "~0.5",
      "grunt-commands": "~0.1",
      "coffee-script": "~1.9.1",
      "load-grunt-tasks": "~3.2.0",
      "glob": "~5.0.10",
      "merge": "~1.2.0"
    },
    "expose-npm-packages": true,
    "require-bower": {
      "jquery": "~2.1.1",
      "jquery-ui": "~1.11.2",
      "backbone": "~1.2.3",
      "backbone.wreqr": "~1.3.5",
      "Jcrop": "~0.9.12",
      "flow.js": "~2.9.0"
    }
  }
}
